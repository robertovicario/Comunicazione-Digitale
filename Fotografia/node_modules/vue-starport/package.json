{
  "name": "vue-starport",
  "version": "0.4.0",
  "packageManager": "pnpm@8.7.4",
  "description": "Shared Vue component across routes with animations",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vue-starport#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vue-starport.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/vue-starport/issues"
  },
  "keywords": [
    "vue",
    "animation",
    "transition"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vueuse/core": "^10.4.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@antfu/ni": "^0.21.8",
    "@types/node": "^20.5.9",
    "bumpp": "^9.2.0",
    "cypress": "^13.1.0",
    "eslint": "^8.48.0",
    "nanoid": "^4.0.2",
    "pkgroll": "^1.11.0",
    "pnpm": "^8.7.4",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.10"
  },
  "scripts": {
    "build": "pkgroll",
    "dev": "pkgroll --watch",
    "play": "vite playground --open",
    "play:build": "vite build playground",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:e2e": "cypress open",
    "release": "bumpp --commit --tag --push && pnpm publish"
  }
}